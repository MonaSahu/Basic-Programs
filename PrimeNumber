using System;
					
public class Prime
{
	public static void Main()
	{
		  	int flag=0,m=0, num;
            Console.WriteLine("Enter the number to find whether number is Prime or not:");
            num = Convert.ToInt32(Console.ReadLine());
			m = num/2;
			for(int i = 2; i<=m ;i++)
			{
				if(num%i == 0)
				{
					Console.WriteLine(num +" is not a prime number");
					flag = 1;
					break;
				}
			}
		if(flag == 0)
		{
			Console.WriteLine(num + " is a prime number");
		}
          
	}
}
