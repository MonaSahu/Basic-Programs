using System;
					
public class Conversion
{
	public static void Main()
	{
		int num ;
		int[] a = new int[10];
		int i;
		Console.WriteLine("Enter the decimal number to find the binary Converstion");
		num = int.Parse(Console.ReadLine());
		for( i =0 ; num>0;i++)
		{
			a[i] = num %2;
			num = num /2;
		}
		for(i= i-1;i>=0;i--)
		{
			Console.Write(a[i]);
		}
	}
}
