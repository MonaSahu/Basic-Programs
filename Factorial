using System;
					
public class Factorial
{
	public static void Main()
	{
		  	int num,fact=1;
            Console.WriteLine("Enter the number to find whether number is Factorial or not:");
            num = Convert.ToInt32(Console.ReadLine());
			for(int i = num ; i>1 ; i--)
			{
				fact = fact *i;
			}
		Console.WriteLine("Factorial of " + num +" is "+ fact);
	}
}
